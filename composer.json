{
    "name": "tecsin/yii2-file-manager",
    "description": "A file manager on elFinder 2.1.20 and TinyMCE 4 for Yii2",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","file","elfinder","wysiwyg","tinymce","tecsin"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Samuel Onyijne",
            "email": "samuel@sajflow.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "tinymce/tinymce": ">=4",
        "studio-42/elfinder": "2.1.20"
    },
    "autoload": {
        "psr-4": {
            "tecsin\\filemanager\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0"
        },
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    }
}
